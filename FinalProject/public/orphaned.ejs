<% include header %>
<style>
table, th, td {
  border: 1px solid black;
}
</style>
<h2>Orphaned Games</h2>
<table>
  <tr>
    <th>Player 1</th>
    <th>Time Created</th>
  </tr>
<% for (let i=0; i < stats.length; i++) { %>
	<tr>
	<td><%= stats[i].player1Name %></td>
	<td><%= new Date(stats[i].lastMoveTime) %></td>
	</tr>
<% } %>
</table>
<% include footer %>
