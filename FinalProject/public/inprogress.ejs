<% include header %>
<style>
table, th, td {
  border: 1px solid black;
}
</style>
<h2>Games In-Progress</h2>
<table>
<tr>
<th>Player 1</th>
<th>Player 2</th>
<th>Last Move</th>
<th>Game State<th>
</tr>
<% for (let i=0; i < stats.length; i++) { %>
	<tr>
	<td><%= stats[i].player1Name %></td>
	<td><%= stats[i].player2Name %></td>
	<td><%= new Date(stats[i].lastMoveTime) %></td>
	<td><%= stats[i].state %></td>
	</tr>
<% } %>
</table>
<% include footer %>
